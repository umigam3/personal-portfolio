{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@UI/*": ["src/components/UI/*"],
      "@icons/*": ["src/icons/*"],
      "@img/*": ["src/assets/img/*"],
      "@file/*": ["src/assets/files/*"]
    }
  }
}